<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/white" />
            <stroke
                android:width="2dp"
                android:color="#EB6733" />
            <corners
                android:radius="@dimen/dp10"
                />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/white" />
            <stroke
                android:width="2dp"
                android:color="#EB6733" />
            <corners
                android:radius="@dimen/dp10"
                />
        </shape>
    </item>
</selector>